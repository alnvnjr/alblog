<div .container>    
    <div .row>
        <div .panel .panel-default>
            <div .panel-heading>
                <h1>
                    All Posts
            <div .panel-body>
                $forall Entity id post <- allPosts
                <div .col-md-6 .col-md-6>
                    <div .cliente>
                        <a href=@{PostDetailsR id}> #{blogPostTitle post}
